<DASSTYLE>
    <STYLESHEET version="1.1">
        <CATEGORY id="default">
            <TYPE id="INS">
                <GLYPH>
                    <TRIANGLE>
                      <DIRECTION>N</DIRECTION>
                      <FGCOLOR>green</FGCOLOR>
                      <HEIGHT>10</HEIGHT>
                      <LINEWIDTH>5</LINEWIDTH>
                    </TRIANGLE>
                </GLYPH>
            </TYPE>
            <TYPE id="DEL">
                <GLYPH>
                    <TRIANGLE>
                      <DIRECTION>S</DIRECTION>
                      <FGCOLOR>red</FGCOLOR>
                      <HEIGHT>10</HEIGHT>
                      <LINEWIDTH>5</LINEWIDTH>
                    </TRIANGLE>
                </GLYPH>
            </TYPE>
            <TYPE id="SNP">
                <GLYPH>
                    <CROSS>
                      <FGCOLOR>blue</FGCOLOR>
                      <HEIGHT>10</HEIGHT>
                    </CROSS>
                </GLYPH>
            </TYPE>
            <TYPE id="DIV">
                <GLYPH>
                    <DOT>
                      <FGCOLOR>purple</FGCOLOR>
                      <BUMP>yes</BUMP>
                      <HEIGHT>10</HEIGHT>
                      <LABEL>yes</LABEL>
                    </DOT>
                </GLYPH>
            </TYPE>
        </CATEGORY>
    </STYLESHEET>
</DASSTYLE>
